<div class="container">
  <div class="panel panel-info">
    <%@feeds.each do |feed| %>
      <div class="panel-body">
        <% if feed.image.blank? %>
          <div class="col-sm-4"><%= image_tag "/uploads/default/default1.jpg", class: "img" %></div>
        <% else %>
          <div class="col-sm-4"><%= image_tag feed.image_url(), class: "img"   %></div>
        <%end%>
        <div class="col-sm-4">
          <%= feed.body %>
        </div>
        <div class="col-sm-4">
          <%= check_box_tag 'bookmark', 'yes' %>
        </div>
      </div>
    <%end%>
  </div>
</div>

<%= will_paginate @feeds %>
   
      


    
  


      